<templateSet group="JavaScript Lessons Templates">
  <template name="google_maps_api_init_map" value="function initMap() {&#10;                map = new google.maps.Map(document.getElementById('map'), {&#10;                    center: {lat: $lat$, lng: $lng$},&#10;                    zoom: 8&#10;                });&#10;            }" description="" toReformat="false" toShortenFQNames="true">
    <variable name="lat" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="lng" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="ECMAScript6" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="google_maps_api_script_async_defer" value=" &lt;script src=&quot;https://maps.googleapis.com/maps/api/js?key=AIzaSyBGmyqfYXWuVBwLJFWiZiXJZ0zCgDawUqA&amp;callback=initMap&quot;&#10;                async defer&gt;&lt;/script&gt;" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="google_map_api_marker" value="var marker = new google.maps.Marker({&#10;    position: $myLatLng$,&#10;    map: $map$,&#10;    title: 'OKTENWEB'&#10;  });" description="" toReformat="false" toShortenFQNames="true">
    <variable name="myLatLng" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="map" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="ECMAScript6" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="xmlHttpRequest" value="// 1. Создаем&#10;var xhr = new XMLHttpRequest();&#10;&#10;// 2. Конфигурируем &#10;xhr.open('GET', 'http://localhost:3000/users, true);&#10;&#10;// 3. Отправляем&#10;xhr.send();" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
